<div class="whois-table form-horizontal">

    <div class="row">
      {{validated-input2 value=model.email errors=errors.email name='admin_email' label='Email' labelWidthClasses='col-sm-3' inputWidthClasses='col-sm-9' class="col-sm-12"  disabled=disabled}}
      {{validated-input2 value=model.name errors=errors.name name='admin_name' label='Name' labelWidthClasses='col-sm-3' inputWidthClasses='col-sm-9' class="col-sm-12" }}
