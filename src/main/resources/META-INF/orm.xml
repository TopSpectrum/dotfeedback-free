<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
                 version="2.0">

    <persistence-unit-metadata>
        <persistence-unit-defaults>
            <entity-listeners>
                <entity-listener class="org.springframework.data.jpa.domain.support.AuditingEntityListener"/>
                <entity-listener class="feedback.jpa.EventListener"/>
            </entity-listeners>
        </persistence-unit-defaults>
    </persistence-unit-metadata>

    <package>feedback</package>

    <entity class="com.topspectrum.billing.dto.BillingPurchase"/>
    <entity class="com.topspectrum.billing.dto.BillingPurchaseOption"/>
    <entity class="com.topspectrum.billing.dto.BillingTransaction"/>
    <entity class="com.topspectrum.billing.dto.CreditCardBillingWallet"/>
    <entity class="com.topspectrum.events.Subscription"/>
    <entity class="com.topspectrum.events.SubscriptionEntry"/>
    <entity class="com.topspectrum.data.dto.Organization"/>

    <entity class="com.topspectrum.feedback.register.data.Reservation"/>
    <entity class="com.topspectrum.feedback.register.data.ReservationCustomer"/>
    <entity class="com.topspectrum.feedback.register.data.PriceTier"/>
    <entity class="com.topspectrum.feedback.register.data.PriceTierRationale"/>
    <entity class="com.topspectrum.feedback.register.data.ReservationDomain"/>

    <entity class="feedback.web.data.MetaData" />
    <entity class="feedback.web.data.HostedContent" />

    <entity class="feedback.web.data.User"/>
    <entity class="feedback.web.data.UserCredential"/>
    <entity class="feedback.web.data.Comment"/>
    <entity class="feedback.web.data.CommentThread"/>
    <entity class="feedback.web.data.Vote"/>
    <entity class="feedback.web.data.DoormanMapping"/>
    <entity class="feedback.web.data.VoteSummary"/>
    <entity class="feedback.web.data.RatingSummary"/>
    <entity class="feedback.web.data.Site"/>
    <entity class="feedback.web.data.Role"/>
    <entity class="feedback.web.data.Review"/>
    <entity class="feedback.web.data.Visit"/>
    <entity class="feedback.web.data.VanityUrl"/>
    <entity class="feedback.web.data.Vendor"/>
    <entity class="feedback.web.data.VendorAccount"/>
    <entity class="feedback.web.data.DoormanMappingOption"/>
    <entity class="feedback.web.data.SiteRingMembership"/>
    <entity class="feedback.web.data.SiteSuggestion"/>
    <entity class="feedback.web.data.PendingVerificationToken"/>
    <entity class="feedback.web.data.Reply"/>
    <entity class="feedback.web.data.Hashtag"/>
    <entity class="feedback.web.data.SiteRing"/>
